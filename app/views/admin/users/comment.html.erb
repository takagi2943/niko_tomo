<% if @user.comment %>
<h5>探究室投稿</h5>
<h5>タグ</h5>
<h5>本文</h5>
<h5>投稿日時</h5>
  <% @labos.each do |labo| %>
    <%= form_with model: labo, url: comments_admin_user_path(labo.id, @user.id), local: true do |f| %>
      <%= f.check_box :body %>
      <div class="tag-form"　style="desplay: none">
        <h5><%= labo.body %></h5>
      </div>
      <h5><%= labo.comments %></h5>
      <h5><%= link_to user.created_at.strftime('%Y/%m/%d %H:%M'), admin_user_comments_path(labo.id, @user.id) %></h5>
    <% end %>
    <%= link_to "チェックしたタグを削除", admin_user_comments_path(labo.id), method: :delete, class: "btn btn-danger fa-solid fa-square-check", remote: true %>
  <% end %>
<% end %>

<h5>音楽共有投稿</h5>
