  <!--    </div>-->
  <!--    <div class="col">-->
        <%#= music_post.body %>
  <!--    </div>-->
  <!--  </div>-->
  <%# end %>
  <h5>探究室の新規投稿</h5>
  <% @labos.each do |labo| %>
    <div class="row">
      <div class="col">
        <h5><%= link_to labo.body, admin_labo_path(labo.id) %></h5>
      </div>
    </div>
  <% end %>
</div>
