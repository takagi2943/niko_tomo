<div class="container">
  <div class="row">
    <h5>サイト利用者数</h5>
    <div class="col">
      <div class="row">
        <h5>計 :<%= @total_users %></h5>
      </div>
      <div class="row">
        <div class="col">
          <h5>登録している人</h5>
        </div>
        <div class="col">
          <h5>計 :<%= @total_active_users %>人</h5>
        </div>
      </div>
      <div class="row">
        <div class="col">
          <h5>退会した人</h5>
        </div>
        <div class="col">
          <h5>計 :<%= @total_inactive_users %>人</h5>
        </div>
      </div>
    </div>
  </div>
  <h5>新規グループ名</h5>
  <% @groups.each do |group| %>
    <div class="row">
      <div class="col">
        <h5><%= link_to group.name, admin_group_path(group.id) %></h5>
      </div>
      <div class="col">
        <h5><%= group.body %></h5>
      </div>
    </div>
  <% end %>
